{
  "name": "src",
  "version": "0.0.0",
  "private": true,
  "scripts": {
  "start": "node ./bin/www",
  "prestart": "npm run build:css && npm run clean:css",
  "build:css:vendor": "cat node_modules/bootstrap/dist/css/bootstrap-reboot.min.css node_modules/bootstrap/dist/css/bootstrap-grid.min.css > public/css/.vendor.css",
  "build:css:ours": "sass public/scss/app.scss public/css/app.custom.css --style=compressed --no-source-map",
  "build:css": "npm run build:css:vendor && npm run build:css:ours && cat public/css/.vendor.css public/css/app.custom.css > public/css/app.css && rm -f public/css/.vendor.css public/css/app.custom.css",
  "watch:css": "sass --watch public/scss/app.scss:public/css/app.css --style=expanded --no-source-map",
  "clean:css": "rm -f public/css/chat.css public/css/main.css public/css/modern.css public/css/theme.css || true"
  },
  "dependencies": {
    "@azure/identity": "^4.11.1",
    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "bootstrap-datepicker": "^1.10.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.4.1",
    "express": "^5.1.0",
    "express-flash": "^0.0.2",
    "express-handlebars": "^8.0.3",
    "express-prom-bundle": "^8.0.0",
    "express-session": "^1.18.2",
    "hbs": "^4.2.0",
    "http-errors": "~2.0.0",
    "i18n": "^0.15.1",
    "moment": "^2.30.1",
    "mongodb": "6.18.0",
    "mongoose": "8.17.1",
    "morgan": "~1.10.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.16.1",
    "prom-client": "^15.1.3",
    "serve-favicon": "^2.5.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-loader": "^8.1.1",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5"
  }
}
